<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>ApiSharp</PackageId>
		<Authors>Burak Öner</Authors>
		<Version>1.0.5</Version>
		<FileVersion>1.0.5</FileVersion>
		<PackageVersion>1.0.5</PackageVersion>
		<AssemblyVersion>1.0.5</AssemblyVersion>
		<Description>Base for Rest Api, Websocket Api, Tcp Socket Api wrappers</Description>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageTags>ApiSharp;Api;Sharp;Client;Tool;Tools;Rest;Web;Websocket;Tcp;Socket;Binance;Huobi;OKX;Okex;FTX</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<NeutralLanguage>en</NeutralLanguage>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageReleaseNotes>
			Version 1.0.5 - 29 Jan 2023
		</PackageReleaseNotes>
		<PackageIcon>ApiSharp.png</PackageIcon>
		<PackageIconUrl />
		<PackageProjectUrl>https://github.com/burakoner/ApiSharp</PackageProjectUrl>
		<RepositoryUrl>https://github.com/burakoner/ApiSharp</RepositoryUrl>
	</PropertyGroup>
	
	<ItemGroup>
		<None Include="ApiSharp.png" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
	</ItemGroup>

</Project>
