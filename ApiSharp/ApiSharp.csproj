<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>ApiSharp</PackageId>
		<Authors>Burak Öner</Authors>
		<Version>1.3.3</Version>
		<FileVersion>1.3.3</FileVersion>
		<PackageVersion>1.3.3</PackageVersion>
		<AssemblyVersion>1.3.3</AssemblyVersion>
		<Description>Base for Rest Api, Websocket Api, Tcp Socket Api Wrappers</Description>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageTags>Binance;Coinbase;Kraken;KuCoin;Bitstamp;Gate;Huobi;OKX;Okex;ApiSharp;Api;Sharp;Coin;Token;BNB;BTC;ETH;Bitcoin;Ether;Ethereum;Rest;Web;Socket;Websocket;Stream;Webstream;Api;Client;Wrapper;Connector;Crypto;Currency;Cryptocurrency;Exchange;Trade;Trading;Spot;Margin;Futures;Derivates;Stock;Options;Swap;</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<NeutralLanguage>en</NeutralLanguage>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageReleaseNotes>
			Version 1.3.3 - 01 May 2023
		</PackageReleaseNotes>
		<PackageIcon>ApiSharp.png</PackageIcon>
		<PackageIconUrl />
		<PackageProjectUrl>https://github.com/burakoner/ApiSharp</PackageProjectUrl>
		<RepositoryUrl>https://github.com/burakoner/ApiSharp</RepositoryUrl>
	</PropertyGroup>
	
	<ItemGroup>
		<None Include="ApiSharp.png" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
